{
  "name": "node-gsheet",
  "version": "1.1.0",
  "description": "Handy functions for reading and merging data into Google Spreadsheet",
  "main": "index.js",
  "bin": "cli.js",
  "scripts": {
    "declaration:build": "rm -f index.d.ts && tsc -p declaration.tsconfig.json",
    "test": "mocha --exit --timeout 30000 --recursive ./test/**/test*",
    "pkg": "pkg . -t node16-win --output ./bin/gsheet.exe"
  },
  "author": "Richard B",
  "license": "MIT",
  "dependencies": {
    "google-spreadsheet": "^3.0.10",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^10.1.0",
    "pkg": "^5.1.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "google",
    "spreadsheet",
    "gsheet"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjellrichard/node-gsheet.git"
  },
  "bugs": {
    "url": "https://github.com/kjellrichard/node-gsheet/issues"
  },
  "homepage": "https://github.com/kjellrichard/node-gsheet#readme"
}
